<title><%= t ".404-page" %></title>
<div class="wrap">
  <div class="h-bg">
    <div class="header">
      <div class="header-right">
        <ul class="follow_icon">
          <li>
            <%= image_tag("image/co-nb.png", alt: "rss feed") %>
          </li>
          <li>
            <%= image_tag("image/lo-pn.png", alt: "rss feed") %>
          </li>
        </ul>
      </div>
      <div class="clear"></div>
      <div class="header-bot">
        <div class="logo">
          <a href="#">
            <%= image_tag("image/logo.png") %>
          </a>
        </div>
        <div class="search">
          <input type="text" class="textbox" value=""
            onfocus="this.value === '';" onblur=  "this.value === '';">
          <button class="gray-button"><span><%= t(".search") %></span>
          </button>
        </div>
        <div class="clear"></div>
      </div>
    </div>

    <div class="menu">
      <div class="top-nav">
        <ul>
          <li class="#">
            <%= link_to t(".home"), "#" %>
          </li>
          <li>
            <%= link_to t(".product"), "#" %>
          </li>
          <li>
            <%= link_to t(".sale"), "#" %>
          </li>
          <li>
            <%= link_to t(".contact"), "#" %>
          </li>
        </ul>
        <div class="clear"></div>
      </div>
    </div>

    <div class="page-not-found">
      <h1><%= link_to t(".404"), "#" %></h1>
    </div>
  </div>
</div>

